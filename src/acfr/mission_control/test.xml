<?xml version="1.0" standalone="no" ?>
<mission>
    <location lat="0.000" lon="0.000" />
    <desc>
        This is a test mission
    </desc>

    <primative>
        <goto>
            <end x="10" y="10" z="1" h="180"/>
            <depth mode = "alititude" />
            <velocity x="1" />
            <timeout t="60" />
            <command device="camera" onoff="on"/>
            <command/>
            <tag>Goto some place</tag>
        </goto>
    </primative>
    <primative>
        <goto>
            <end x="100" y="100" z="1"/>
            <velocity x="1" />
            <timeout t="60" />
        </goto>
    </primative>
    <primative>
        <line>
            <start x="100" y="100" z="1"/>
            <end x="200" y="100" z="1" />
            <start_velocity x = "1" />
            <end_velocity x = "1" />
            <command device="camera" onoff="on" freq="1.5" time="10000" />
            <timeout t="60" />
        </line>
    </primative>
    <primative>
        <command>
            <command device="camera" onoff="off"/>
        </command>
    </primative>    
    <primative>
        <grid>
            <center x="200" y="150" z="2"/>
            <length l="25" />
            <width w="25" />
            <lines lines = "10" />
            <rotation r = "45" />
            <command device="camera" onoff="on"/>
            <timeout t="60" />
            <depth mode = "altitude" />
            <velocity x="1" />
        </grid>
    </primative>

</mission>
