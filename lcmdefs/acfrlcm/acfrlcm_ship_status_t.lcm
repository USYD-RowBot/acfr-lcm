package acfrlcm;

struct ship_status_t
{
    int64_t utime;
    
    int8_t  ship_id;
    string name;
    
    // all in radians (including lat/lon) for consistency with other sensor
    // data types
    double heading;   
    double roll;      
    double pitch;     
    double latitude;
    double longitude;
}
